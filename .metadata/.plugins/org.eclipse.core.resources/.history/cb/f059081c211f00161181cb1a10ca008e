package com.bookstore.restful;

public class User {
	private String name;

	
	public User() {
		// TODO Auto-generated constructor stub
	}
	
	public User(String name) {
		this.setName(name);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
